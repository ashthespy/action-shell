{
    "problemMatcher": [
      {
        "owner": "shellcheck-warning",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+):\\s(note|warning):\\s(.*)\\s\\[(SC\\d+)\\]$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 5,
            "code": 6
          }
        ]
      },
      {
        "owner": "shellcheck-error",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+):\\s(error):\\s(.*)\\s\\[(SC\\d+)\\]$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5,
            "code": 6
          }
        ]
      }
    ]
  }